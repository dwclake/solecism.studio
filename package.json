{
  "name": "solecism.studio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --watch --hot src/main.tsx",
    "build": "bun build ./index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/main.tsx"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "react": "^19",
    "react-dom": "^19",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-redux": "^7.1.34",
    "@types/react-router": "^5.1.20",
    "@types/redux": "^3.6.0"
  }
}
